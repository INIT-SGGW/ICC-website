{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:web": "pnpm run build --filter web",
    "build:admin": "pnpm run build --filter admin",
    "build:backend": "pnpm run build --filter backend",
    "dev": "turbo dev",
    "dev:web": "pnpm run dev --filter web",
    "dev:admin": "pnpm run dev --filter admin",
    "dev:api": "pnpm run dev --filter api",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "braces": "3.0.3",
      "cross-spawn": "7.0.5",
      "micromatch": "4.0.8",
      "nanoid": "3.3.8"
    }
  },
  "name": "init-coding-challenge"
}
